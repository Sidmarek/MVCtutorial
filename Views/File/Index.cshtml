@model MVCtutorial.File.Models.FileFormModel
@{ 
    ViewBag.Title = "File Managment System(FMS)";
}

<h2>@ViewBag.Title.</h2>

@Html.ActionLink("Create XML File", "preXMLForm", "File")<br>

@ViewBag.count<br>
<ul>
@foreach(var number in ViewBag.Numbers) {
    <li>@number</li>
}
</ul>
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}